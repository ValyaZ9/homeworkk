def backspace_compare(first: str, second: str)-> bool:
        def process_string(string: str) -> str:
            result = []
            for char in string:
                if char == '#':
                    if result:
                        result.pop()
                else:
                    result.append(char)
            return ''.join(result)

        return process_string(first) == process_string(second)
